{
  "name": "fets",
  "version": "0.8.3",
  "type": "module",
  "description": "TypeScript HTTP Framework focusing on e2e type-safety, easy setup, performance & great developer experience",
  "repository": {
    "type": "git",
    "url": "ardatan/fets",
    "directory": "packages/fets"
  },
  "author": "Arda TANRIKULU <ardatanrikulu@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "dependencies": {
    "@sinclair/typebox": "^0.33.0",
    "@whatwg-node/cookie-store": "^0.2.0",
    "@whatwg-node/fetch": "^0.9.4",
    "@whatwg-node/server": "^0.9.15",
    "hotscript": "^1.0.11",
    "json-schema-to-ts": "^3.0.0",
    "qs": "^6.11.2",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/qs": "^6.9.8",
    "express": "^4.18.2",
    "html-minifier-terser": "7.2.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
